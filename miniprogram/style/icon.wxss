/* 阿里字体图标样式 */

@font-face {
  font-family: 'iconfont';  /* project id 1205494 */
  src: url('//at.alicdn.com/t/font_1205494_oz2s7299c.eot');
  src: url('//at.alicdn.com/t/font_1205494_oz2s7299c.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1205494_oz2s7299c.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1205494_oz2s7299c.woff') format('woff'),
  url('//at.alicdn.com/t/font_1205494_oz2s7299c.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1205494_oz2s7299c.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

